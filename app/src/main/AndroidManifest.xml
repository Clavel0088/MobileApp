            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.InscriptionActivity" />
        <activity android:name=".activity.ProfilActivity" />
        <activity android:name=".activity.CallHistoryActivity" />
        <activity android:name=".activity.OfferActivity" />
        <activity android:name=".activity.TransactionActivity" />
